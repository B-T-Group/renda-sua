    "lint:backend": "nx lint backend",
    "lint:cdk": "nx lint cdk",
    "typecheck": "nx run-many --target=typecheck --all",
    "typecheck:frontend": "nx typecheck frontend",
    "typecheck:backend": "nx typecheck backend",
    "typecheck:cdk": "nx typecheck cdk",
    "dev": "nx run-many --target=serve --all --parallel",
    "dev:frontend": "nx serve frontend",
    "dev:backend": "nx serve backend",
    "dev:backend:fast": "nx run backend:dev",
    "docker:build:backend": "nx run backend:docker-build",
    "cdk:deploy": "cd apps/cdk && cdk deploy",
    "cdk:deploy:dev": "cd apps/cdk && cdk deploy --context environment=dev",
    "cdk:deploy:staging": "cd apps/cdk && cdk deploy --context environment=staging",
    "cdk:deploy:prod": "cd apps/cdk && cdk deploy --context environment=production",
    "cdk:destroy": "cd apps/cdk && cdk destroy",
    "cdk:diff": "cd apps/cdk && cdk diff",
    "cdk:synth": "cd apps/cdk && cdk synth",
    "cdk:bootstrap": "cd apps/cdk && cdk bootstrap",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
