  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:frontend": "nx build frontend",
    "build:backend": "nx build backend",
    "build:cdk": "nx build cdk",
    "serve": "nx run-many --target=serve --all --parallel",
    "serve:frontend": "nx serve frontend",
    "serve:backend": "nx serve backend",
    "start": "nx run-many --target=start --all --parallel",
    "start:frontend": "nx serve frontend",
    "start:backend": "nx start backend",
    "test": "nx run-many --target=test --all",
    "test:frontend": "nx test frontend",
    "test:backend": "nx test backend",
    "test:cdk": "nx test cdk",
