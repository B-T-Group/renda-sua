{
  "name": "cdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cdk/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/cdk",
        "main": "apps/cdk/src/main.ts",
        "tsConfig": "apps/cdk/tsconfig.json",
        "assets": ["apps/cdk/src"]
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "cdk:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/cdk/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "typecheck": {
      "executor": "@nx/js:tsc",
      "options": {
        "tsConfig": "apps/cdk/tsconfig.json"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cdk deploy",
        "cwd": "apps/cdk"
      }
    },
    "destroy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cdk destroy",
        "cwd": "apps/cdk"
      }
    },
    "diff": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cdk diff",
        "cwd": "apps/cdk"
      }
    },
    "synth": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cdk synth",
        "cwd": "apps/cdk"
      }
    }
  },
  "tags": []
}
